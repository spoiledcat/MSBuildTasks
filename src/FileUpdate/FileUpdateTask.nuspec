<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <authors>$authors$</authors>
    <owners>$owners$</owners>
    <icon>images\icon.png</icon>
    <developmentDependency>true</developmentDependency>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">$license$</license>
    <projectUrl>$url$</projectUrl>
    <description>$description$</description>
    <summary>$summary$</summary>
    <copyright>$copyright$</copyright>
    <repository type="git" url="https://github.com/spoiledcat/MSBuildTasks" commit="$commit$" />
    <dependencies>
      <group targetFramework=".NETFramework4.7.1">
        <dependency id="Microsoft.Build.Framework" version="17.12.6" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Build.Utilities.Core" version="17.12.6" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETCoreApp3.1">
        <dependency id="Microsoft.Build.Framework" version="17.12.6" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Build.Utilities.Core" version="17.12.6" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NET8.0">
        <dependency id="Microsoft.Build.Framework" version="17.12.6" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Build.Utilities.Core" version="17.12.6" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Threading.Thread" targetFramework=".NETFramework4.7.1" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="$name$.targets" target="build\$id$.targets" />
    <file src="$buildDir$net471\$name$.dll" target="build\net471" />
    <file src="$buildDir$netcoreapp3.1\$name$.dll" target="build\netcoreapp3.1" />
    <file src="$buildDir$net8.0\$name$.dll" target="build\net8.0" />
    <file src="$commonDir$icon.png" target="images\icon.png" />
    <file src="_._" target="lib\net471" />
    <file src="_._" target="lib\netcoreapp3.1" />
    <file src="_._" target="lib\net8.0" />
  </files>
</package>